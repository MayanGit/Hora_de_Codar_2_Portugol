programa {
  funcao inicio() {
    inteiro n1
    inteiro n2 

     escreva ("Informe um número: ")
     leia (n1)
     escreva ("Informe outro número: ")
     leia (n2)

    se (n1>n2){
      escreva ("O número maior é o: ",n1)
    }

    senao {
      escreva ("O número maior é o: ",n2)
    }

  }
}
