programa {
  funcao inicio() {
    real n1

     escreva ("Informe um número: ")
     leia (n1)

    se (n1<0){
      escreva ("O número informado é Negativo")
    }
    senao se (n1>0){
      escreva ("O número informado é Positivo")
    }
    senao {
      escreva ("O número informado é o 0")
    }

  }
}
